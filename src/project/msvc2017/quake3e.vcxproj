  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.targets" />
  <ImportGroup Label="ExtensionTargets">
  </ImportGroup>
  <!-- Compile all shaders directly in MSBuild -->
  <PropertyGroup>
    <VulkanSDKPath>C:\VulkanSDK\1.3.290.0</VulkanSDKPath>
    <GlslangValidator>$(VulkanSDKPath)\Bin\glslangValidator.exe</GlslangValidator>
    <ShaderRootDir>$(ProjectDir)..\..\engine\renderer\shaders</ShaderRootDir>
    <ShaderOutputDir>$(ProjectDir)..\..\..\baseq3\shaders</ShaderOutputDir>
  </PropertyGroup>

  <!-- RTX Ray Tracing Shaders -->
  <Target Name="CompileRTXShaders" BeforeTargets="Build">
    <Message Text="Compiling RTX ray tracing shaders..." Importance="high" />
    <MakeDir Directories="$(ShaderOutputDir)\rtx" />
    
    <!-- Compile raygen shader -->
    <Exec Command="&quot;$(GlslangValidator)&quot; -V --target-env vulkan1.2 -S rgen &quot;$(ShaderRootDir)\rtx\raygen.rgen&quot; -o &quot;$(ShaderOutputDir)\rtx\raygen.spv&quot;"
          ContinueOnError="false" />
    
    <!-- Compile miss shaders -->
    <Exec Command="&quot;$(GlslangValidator)&quot; -V --target-env vulkan1.2 -S rmiss &quot;$(ShaderRootDir)\rtx\miss.rmiss&quot; -o &quot;$(ShaderOutputDir)\rtx\miss.spv&quot;"
          ContinueOnError="false" />
    <Exec Command="&quot;$(GlslangValidator)&quot; -V --target-env vulkan1.2 -S rmiss &quot;$(ShaderRootDir)\rtx\shadow.rmiss&quot; -o &quot;$(ShaderOutputDir)\rtx\shadow.spv&quot;"
          ContinueOnError="false" />
    
    <!-- Compile closesthit shader -->
    <Exec Command="&quot;$(GlslangValidator)&quot; -V --target-env vulkan1.2 -S rchit &quot;$(ShaderRootDir)\rtx\closesthit.rchit&quot; -o &quot;$(ShaderOutputDir)\rtx\closesthit.spv&quot;"
          ContinueOnError="false" />
  </Target>

  <!-- TAA and Compute Shaders -->
  <Target Name="CompileTAAShaders" BeforeTargets="Build">
    <Message Text="Compiling TAA and compute shaders..." Importance="high" />
    <MakeDir Directories="$(ShaderOutputDir)\compute" />
    
    <!-- Compile TAA resolve shader -->
    <Exec Command="&quot;$(GlslangValidator)&quot; -V --target-env vulkan1.2 -S comp &quot;$(ShaderRootDir)\compute\taa_resolve.comp&quot; -o &quot;$(ShaderOutputDir)\compute\taa_resolve.spv&quot;"
          ContinueOnError="false" />
    
    <!-- Compile TAA sharpen shader -->
    <Exec Command="&quot;$(GlslangValidator)&quot; -V --target-env vulkan1.2 -S comp &quot;$(ShaderRootDir)\compute\taa_sharpen.comp&quot; -o &quot;$(ShaderOutputDir)\compute\taa_sharpen.spv&quot;"
          ContinueOnError="false" />
    
    <!-- Compile GPU culling shader -->
    <Exec Command="&quot;$(GlslangValidator)&quot; -V --target-env vulkan1.2 -S comp &quot;$(ShaderRootDir)\compute\gpu_culling.comp&quot; -o &quot;$(ShaderOutputDir)\compute\gpu_culling.spv&quot;"
          ContinueOnError="false" />
  </Target>

  <!-- GLSL Vertex and Fragment Shaders -->
  <Target Name="CompileGLSLShaders" BeforeTargets="Build">
    <Message Text="Compiling GLSL shaders..." Importance="high" />
    <MakeDir Directories="$(ShaderOutputDir)\glsl" />
    
    <!-- Vertex shaders -->
    <Exec Command="&quot;$(GlslangValidator)&quot; -V -S vert &quot;$(ShaderRootDir)\glsl\color.vert&quot; -o &quot;$(ShaderOutputDir)\glsl\color_vert.spv&quot;" ContinueOnError="false" />
    <Exec Command="&quot;$(GlslangValidator)&quot; -V -S vert &quot;$(ShaderRootDir)\glsl\gamma.vert&quot; -o &quot;$(ShaderOutputDir)\glsl\gamma_vert.spv&quot;" ContinueOnError="false" />
    <Exec Command="&quot;$(GlslangValidator)&quot; -V -S vert &quot;$(ShaderRootDir)\glsl\fog.vert&quot; -o &quot;$(ShaderOutputDir)\glsl\fog_vert.spv&quot;" ContinueOnError="false" />
    <Exec Command="&quot;$(GlslangValidator)&quot; -V -S vert &quot;$(ShaderRootDir)\glsl\dot.vert&quot; -o &quot;$(ShaderOutputDir)\glsl\dot_vert.spv&quot;" ContinueOnError="false" />
    
    <!-- Fragment shaders -->
    <Exec Command="&quot;$(GlslangValidator)&quot; -V -S frag &quot;$(ShaderRootDir)\glsl\color.frag&quot; -o &quot;$(ShaderOutputDir)\glsl\color_frag.spv&quot;" ContinueOnError="false" />
    <Exec Command="&quot;$(GlslangValidator)&quot; -V -S frag &quot;$(ShaderRootDir)\glsl\gamma.frag&quot; -o &quot;$(ShaderOutputDir)\glsl\gamma_frag.spv&quot;" ContinueOnError="false" />
    <Exec Command="&quot;$(GlslangValidator)&quot; -V -S frag &quot;$(ShaderRootDir)\glsl\fog.frag&quot; -o &quot;$(ShaderOutputDir)\glsl\fog_frag.spv&quot;" ContinueOnError="false" />
    <Exec Command="&quot;$(GlslangValidator)&quot; -V -S frag &quot;$(ShaderRootDir)\glsl\dot.frag&quot; -o &quot;$(ShaderOutputDir)\glsl\dot_frag.spv&quot;" ContinueOnError="false" />
    <Exec Command="&quot;$(GlslangValidator)&quot; -V -S frag &quot;$(ShaderRootDir)\glsl\blur.frag&quot; -o &quot;$(ShaderOutputDir)\glsl\blur_frag.spv&quot;" ContinueOnError="false" />
    <Exec Command="&quot;$(GlslangValidator)&quot; -V -S frag &quot;$(ShaderRootDir)\glsl\bloom.frag&quot; -o &quot;$(ShaderOutputDir)\glsl\bloom_frag.spv&quot;" ContinueOnError="false" />
    <Exec Command="&quot;$(GlslangValidator)&quot; -V -S frag &quot;$(ShaderRootDir)\glsl\blend.frag&quot; -o &quot;$(ShaderOutputDir)\glsl\blend_frag.spv&quot;" ContinueOnError="false" />
  </Target>
  <Target Name="CopyCompiledShaders" AfterTargets="Build">
    <Message Text="Copying compiled shaders to output directory..." Importance="high" />
    <!-- Create shader directories in output -->
    <MakeDir Directories="$(OutDir)baseq3\shaders\rtx" />
    <MakeDir Directories="$(OutDir)baseq3\shaders\compute" />
    <MakeDir Directories="$(OutDir)baseq3\shaders\glsl" />
    
    <!-- Copy RTX shaders -->
    <ItemGroup>
      <RTXShaders Include="$(ProjectDir)..\..\..\baseq3\shaders\rtx\*.spv" />
    </ItemGroup>
    <Copy SourceFiles="@(RTXShaders)" DestinationFolder="$(OutDir)baseq3\shaders\rtx" SkipUnchangedFiles="true" />
    
    <!-- Copy Compute shaders (TAA, GPU culling) -->
    <ItemGroup>
      <ComputeShaders Include="$(ProjectDir)..\..\..\baseq3\shaders\compute\*.spv" />
    </ItemGroup>
    <Copy SourceFiles="@(ComputeShaders)" DestinationFolder="$(OutDir)baseq3\shaders\compute" SkipUnchangedFiles="true" />
    
    <!-- Copy GLSL shaders if they exist -->
    <ItemGroup>
      <GLSLShaders Include="$(ProjectDir)..\..\..\baseq3\shaders\glsl\*.spv" />
    </ItemGroup>
    <Copy SourceFiles="@(GLSLShaders)" DestinationFolder="$(OutDir)baseq3\shaders\glsl" SkipUnchangedFiles="true" ContinueOnError="true" />
    
    <Message Text="Shader copy complete." Importance="high" />
  </Target>
</Project>